package com.test02;

public class Student {
	String number;
	String major;
	
	public Student() {}
	public Student(String name, int age, int height, int weight, String number, String major) {
		super();
		this.number=number;
		this.major=major;
	}
	
	@Override 	
	public String toString() {
		return super.toString()+"number : "+number+", major : "+major;
	}
	
}
