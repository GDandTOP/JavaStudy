package Employee;

public class Permanent extends Employee {

	private int pay;
	
	public Permanent(int id, String name, int pay) {
		super(id, name);
		this.pay = pay;
		// TODO Auto-generated constructor stub
	}
	
	@Override
	public String toString() {
		return "사번 : "+this.getId()+", 이름 : "+this.getName()+", 급여 : "+pay;
	}
	

	
	public int getPay() {
		return pay;
	}

}
