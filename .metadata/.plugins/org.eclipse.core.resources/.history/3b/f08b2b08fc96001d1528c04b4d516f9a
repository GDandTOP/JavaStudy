package com.test04;

public class Cargoplane extends Plane{

	Cargoplane() {}
	
	Cargoplane(String planeName, int fuelSize) {
		super(planeName,fuelSize);
	}

	
	@Override
	public void flight(int distance) {
		if (fuelSize>(distance*5)) {
			fuelSize-=(distance*5);
		}
		
	}
	
	

	
}
