package com.test04;

public class PlaneTest {

	public static void main(String[] args) {
		Plane a = new Airplane("L777",1000);
		Plane b = new Cargoplane("C50",1000);
		
	
		
		System.out.println("Plane fuelSize");
		System.out.println("----------------------");
		System.out.println(a.planeName+" "+a.fuelSize);
		System.out.println(b.planeName+" "+b.fuelSize);
		
		a.flight(100);
		b.flight(100);
		
		System.out.println(100 +" 운항");
		
		
		System.out.println();
		System.out.println("Plane fuelSize");
		System.out.println("----------------------");
		System.out.println(a.planeName+" "+a.fuelSize);
		System.out.println(b.planeName+" "+b.fuelSize);
		
		a.refule(200);
		b.refule(200);
		System.out.println(200 +" 주유");
		
		System.out.println();
		System.out.println("Plane fuelSize");
		System.out.println("----------------------");
		System.out.println(a.planeName+" "+a.fuelSize);
		System.out.println(b.planeName+" "+b.fuelSize);
		
	}

}
